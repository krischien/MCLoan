	<queryString>
		<![CDATA[SELECT
c.id,
c.account_no,
c.firstname,
c.middlename,
c.lastname,
c.gender,
c.civil_Status,
c.birthdate,
c.mobile_no,
c.landline,
c.email_address,
c.business_address,
c.street,
c.barangay,
c.city_province,
c.cedula,
c.place_issued,
c.date_issued,
p.others,
p.type,
p.pension_no,
p.`name`,
p.bank_account_no,
p.monthly_pension,
p.date_remittance,
c.spouse_name,
c.spouse_birthdate,
c.spouse_date_of_death,
r.name as charname,
r.address as charaddress,
r.contact_no as charcontact
FROM clients c
LEFT JOIN banks b  ON (c.client_bank= b.id)
LEFT JOIN pensions p ON (c.id = p.client_id )
LEFT JOIN character_reference r ON (c.id = r.client_id )
WHERE c.id=$P{id}]]>
